def main():
    filename = 'train.csv'
    data = open(filename, 'r')
    contents = data.readlines()
    row = []
    for i in range(len(contents)-1):
        row.append(contents[i].split('"'))
        del row[i][0:1]
        del row[i][1:2]
        for a in range(10):
            del row[i][a+4:a+5]
 
    del row[0][2:3]
    del row[0][13:14]
    return row

def first(row):
    header = row[0]
    print(header)


if __name__ == '__main__':
    row = main()
    first(row)
